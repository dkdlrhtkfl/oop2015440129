
public class Point {
	
	public double x;
	public double y;

	public double distance() {
		double result;
		result = Math.sqrt(x*x + y*y);
		
		return result;
	}
	
	
	
	
	
	public Point move(Point p) {
		//temp 변수 선
		double a;
		double b;

		//포인트 p만큼 이동
		a = this.x + p.x;
		b = this.y + p.y;
		
		//새로운 포인트 생성
		Point pnt = new Point();
		pnt.x = a;
		pnt.y = b;
		
		//새로운 포인트 반환
		return pnt;
		
	}

}
